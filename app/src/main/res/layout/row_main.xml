<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="12dp">

    <TextView
        android:id="@+id/name_listextView"
        android:layout_width="260dp"
        android:layout_height="24dp"
        android:layout_marginStart="112dp"
        android:textColor="@color/Orange_700"
        android:layout_marginTop="16dp"
        android:text="Name"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="112dp" />

    <TextView
        android:id="@+id/name_listextView2"
        android:layout_width="66dp"
        android:layout_height="24dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:textColor="@color/Orange_700"
        android:text="Nombre"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="28dp" />

    <TextView
        android:id="@+id/position_textview"
        android:layout_width="260dp"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textColor="@color/Orange_700"
        app:layout_constraintStart_toStartOf="@+id/name_listextView"
        app:layout_constraintTop_toBottomOf="@+id/name_listextView" />

    <TextView
        android:id="@+id/position_textview2"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        android:text="Id"
        android:textColor="@color/Orange_700"
        app:layout_constraintStart_toStartOf="@+id/name_listextView2"
        app:layout_constraintTop_toBottomOf="@+id/name_listextView2" />
</androidx.constraintlayout.widget.ConstraintLayout>